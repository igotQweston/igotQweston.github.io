{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "weight",
          "name": "weight",
          "type": "number"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "contributors",
          "name": "contributors",
          "type": "choice",
          "choices": [
            "Fawkes ND"
          ]
        },
        {
          "title": "pinned",
          "name": "pinned",
          "type": "boolean"
        },
        {
          "title": "seo",
          "name": "seo",
          "type": "fields",
          "fields": [
            {
              "title": "title",
              "name": "title",
              "type": "string"
            },
            {
              "title": "description",
              "name": "description",
              "type": "string"
            },
            {
              "title": "canonical",
              "name": "canonical",
              "type": "string"
            },
            {
              "title": "noindex",
              "name": "noindex",
              "type": "boolean"
            }
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects"
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.website.host": "https://igotqweston.github.io"
}